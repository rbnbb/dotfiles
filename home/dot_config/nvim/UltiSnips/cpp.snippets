snippet incl "" b
#include<${1:iostream}>
endsnippet

snippet lr "paranthesis" i
(${1:${VISUAL}})$0
endsnippet

snippet lra "paranthesis" i
<${1:${VISUAL}}>$0
endsnippet

snippet lrb "" i
{
	${1:${VISUAL}}
}$0
endsnippet

snippet [ "paranthesis" i
[${1:${VISUAL}}]$0
endsnippet
 
snippet "\"\"" "" irA 
"${1:${VISUAL}}"$0
endsnippet

snippet for "for loop" b
for(int ${1:i};${2:$1<=$3};$1++)
{
	$0
}
endsnippet
 
snippet printf "printf automatic" bA
printf("$1\n",$2);
$0
endsnippet
